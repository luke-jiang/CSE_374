echo "Hello world"
passwd
date
echo ~
cd ~
ls
mkdir temp
mv temp files
cd files
date > now
cp now before
ls -l
echo "" > temp
mv temp test
cd ..
rm -r files
echo "It's a secret"! > secretfile
# change the permission so that no one can write it
chmod a-w secretfile
ls -l secretfile
# add more content into secretfile
cat <<< "some content" >> secretfile
# change the permission so that no one can read it
chmod a-r secretfile
ls -l secretfile
cat secretfile

# display version of Linux kernel
cat /proc/version
# list of all my processes
ps -u yuxuaj
grep yuxuaj /etc/passwd
